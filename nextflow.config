params {
	repsProcessA = 10
	processATimeRange = "1-2"
	filesProcessA = 1
	processATimeBetweenFileCreationInSecs = 0

	processBTimeRange = "2-3"
	processBWriteToDiskMb = 10

	processCTimeRange = "2-3"
	processDTimeRange = "2-3"

	output = "results"
}


process {
  withName: processA {
  	cpus =  1
  	memory = "4 GB"
  }
  
  withName: processB {
      cpus = 20
      memory = "20 GB"
  }
}


process {
	container = "ubuntu:18.10"
	errorStrategy = "retry"
	maxRetries = 5
	executor = "awsbatch"
    queue = "nf-batch-4"
}

aws.batch.cliPath = '/home/ec2-user/miniconda/bin/aws'
aws.batch.fetchInstanceType = true


